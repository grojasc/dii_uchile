           FROM (((pago_alumno
             JOIN orden_facturacion ON ((pago_alumno.id_pago_alumno = orden_facturacion.id_pago_alumno)))
             JOIN cuotas_alumno ON ((pago_alumno.id_pago_alumno = cuotas_alumno.id_pago_alumno)))
             JOIN cuota ON ((cuota.id_cuota = cuotas_alumno.id_cuota)))
