     JOIN cuotas_empresa ON ((pago_empresa.id_pago_empresa = cuotas_empresa.id_pago_empresa)))
     JOIN cuota ON ((cuota.id_cuota = cuotas_empresa.id_cuota)))
  WHERE ((pago_empresa.id_estado_envio_of_a_softland = 3) AND (pago_empresa.total_empresa > 0) AND ((cuota.numero_factura IS NULL) OR (cuota.numero_factura = ''::text)))
UNION
 SELECT pago_otic.id_pago_otic AS id_pago,
    orden_facturacion.id_of,
    1 AS numero_cuota,
    NULL::integer AS id_cuota,
    (orden_facturacion.id_of || 'O'::text) AS cadena_para_pasar_a_softland,
