           FROM (op_of
             JOIN orden_validacion_academica ON ((op_of.id_va = orden_validacion_academica.id_va)))) k(id_va, id_of, fecha_registro, id_va_1, id_programa, id_alumno, id_dir_aa, id_vendedor, id_cordinador_aa, id_empresa, id_ejecutivo_empresa, vb_vendedor, vb_cordinadoraa, vb_directoraa, comentario_vendedor, comentario_cordinadoraa, comentario_directoraa, fecha_vb_vendedor, fecha_vb_cordinadoraa, fecha_vb_directoraa, fecha_registro_1, motivo_rechazo, paga_externo, fecha_envio_mail, contenido_mail, oyente) ON ((k.id_alumno = persona.id_persona)));]]></view_script>
