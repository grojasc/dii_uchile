		<view name="v_usuarios_unico_login" >
			<view_script><![CDATA[CREATE VIEW ${schema}.${name} AS  SELECT x.id_persona,
    x.rol,
    persona.id_comuna,
    persona.id_nacionalidad,
    persona.id_pais,
    persona.nombre1,
    persona.nombre2,
    persona.apellido1,
    persona.apellido2,
    persona.rut,
    persona.direccion,
    persona.telefono,
    persona.celular,
    persona.email,
    persona.fecha_nacimiento,
    persona.fecha_registro,
    persona.contrasegna,
    persona.sexo,
    persona.link_foto,
    persona.username,
    persona.mail_empresa,
    persona.pasaporte,
    persona.autovalidar_pagos_rrhh,
    persona.id_convenios_y_honorarios,
    persona.id_persona_contralor_rrhh,
    persona.enviar_apas_directo_a_sga,
    persona.destinatario_compra,
    persona.origen_compra,
    persona.id_persona_contralor_apas,
    persona.id_persona_convenios_segunda,
    persona.anexo,
    persona.anexo2,
    persona.telefono2,
    persona.puede_tener_convenios,
    persona.puede_tener_apas,
    persona.se_le_puede_pagar_comision,
    persona.id_origen,
    persona.ciudad_nacimiento,
    persona.id_en_calidad_de,
    persona.puede_tener_pago_menor,
    persona.puede_tener_tci,
    persona.puede_hacer_pagos_fia,
    persona.id_persona_firma_ac,
    persona.cumpleanos_publico,
    persona.debe_cambiar_contrasegna,
    persona.patente_vehiculo,
    persona.enviado_al_crm,
    persona.id_persona_firma_ac_2,
    persona.recibe_correo_bandeja,
    persona.recibe_comision_venta,
    persona.ver_pagos_realizados,
    persona.ver_pagos_por_realizar,
    persona.id_persona_convenios_proyecto,
    persona.id_persona_convenios_proyecto_2,
    persona.id_persona_contralor_proyecto,
    persona.fecha_inicio_comision_venta,
    persona.enviar_email_pago_proveedor,
    persona.direccion_oficina,
    persona.ambito_docencia,
    persona.certificado,
    replace(persona.rut, '.'::text, ''::text) AS rut_sin_punto
   FROM (( SELECT v_usuarios_activos.id_persona,
            v_usuarios_activos.rol
           FROM v_usuarios_activos
          WHERE (v_usuarios_activos.id_persona IN ( SELECT a.id_persona
                   FROM ( SELECT v_usuarios_activos_1.id_persona,
                            count(*) AS c
                           FROM v_usuarios_activos v_usuarios_activos_1
                          GROUP BY v_usuarios_activos_1.id_persona) a
                  WHERE (a.c = 1)))
        UNION
         SELECT DISTINCT v_usuarios_activos.id_persona,
            'masdeuno'::text AS rol
           FROM v_usuarios_activos
          WHERE (NOT (v_usuarios_activos.id_persona IN ( SELECT a.id_persona
                   FROM ( SELECT v_usuarios_activos_1.id_persona,
                            count(*) AS c
                           FROM v_usuarios_activos v_usuarios_activos_1
                          GROUP BY v_usuarios_activos_1.id_persona) a
                  WHERE (a.c = 1))))
        UNION
         SELECT DISTINCT persona_1.id_persona,
            'sin_rol'::text AS rol
           FROM persona persona_1
          WHERE (NOT (persona_1.id_persona IN ( SELECT v_usuarios_activos.id_persona
                   FROM v_usuarios_activos)))) x
     JOIN persona USING (id_persona));]]></view_script>
