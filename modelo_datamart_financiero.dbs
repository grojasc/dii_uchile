     JOIN cuota ON ((cuota.id_cuota = cuotas_alumno.id_cuota)))
  WHERE ((pago_alumno.id_estado_envio_of_a_softland = 3) AND (pago_alumno.total_alumno > 0) AND ((cuota.numero_factura IS NULL) OR (cuota.numero_factura = ''::text)))
UNION
 SELECT pago_empresa.id_pago_empresa AS id_pago,
    orden_facturacion.id_of,
    cuota.numero_cuota,
