		<view name="v_documentos_tributarios" >
			<view_script><![CDATA[CREATE VIEW ${schema}.${name} AS  SELECT v1.numero_factura_referenciada,
    v1.tipo_detalle,
    v1.id_tipo_documento_tributario,
    v1.beneficiario_documento,
    v1.monto_doc,
    v1.numero_doc,
    v1.fecha_lee_doc,
    v1.id_of,
    v1.numero_cc,
    v1.iva,
        CASE
            WHEN (v1.tipo_detalle = ANY (ARRAY[1, 2, 3])) THEN ( SELECT concat((auxiliar_doc_tributario.fecha_documento)::text, '###', auxiliar_doc_tributario.num_comprobante_informat, '###', (auxiliar_doc_tributario.monto_documento)::text, '###', auxiliar_doc_tributario.id_tipo_documento_tributario) AS concat
               FROM auxiliar_doc_tributario
              WHERE ((auxiliar_doc_tributario.folio = v1.numero_doc) AND (auxiliar_doc_tributario.id_tipo_documento_tributario = v1.id_tipo_documento_tributario)))
            WHEN (v1.tipo_detalle = 4) THEN ( SELECT concat((auxiliar_doc_tributario.fecha_documento)::text, '###', auxiliar_doc_tributario.num_comprobante_informat, '###', (v1.monto_doc)::text, '###', auxiliar_doc_tributario.id_tipo_documento_tributario) AS concat
               FROM auxiliar_doc_tributario
              WHERE ((auxiliar_doc_tributario.folio = v1.numero_doc) AND (auxiliar_doc_tributario.id_tipo_documento_tributario = v1.id_tipo_documento_tributario)))
            ELSE concat(v1.fecha_lee_doc, '###', v1.num_comprobante_informat, '###', v1.monto_doc, '###', v1.id_tipo_documento_tributario, '###')
        END AS info_adicional
   FROM ( SELECT NULL::text AS numero_factura_referenciada,
            1 AS tipo_detalle,
            0 AS iva,
            cuota.id_tipo_documento_tributario,
            ( SELECT concat(rtrim((pago_alumno.nombre)::text, ' '::text), '######', pago_alumno.rut) AS concat
                   FROM pago_alumno
                  WHERE (pago_alumno.id_pago_alumno = cuotas_alumno.id_pago_alumno)) AS beneficiario_documento,
            cuota.total_cuota AS monto_doc,
            cuota.numero_factura AS numero_doc,
            cuota.fecha_pago AS fecha_lee_doc,
            ( SELECT orden_facturacion.id_of
                   FROM orden_facturacion
                  WHERE (orden_facturacion.id_pago_alumno = cuotas_alumno.id_pago_alumno)) AS id_of,
            ( SELECT programa.numero_cc
                   FROM (programa
                     JOIN orden_facturacion ON ((orden_facturacion.id_programa = programa.id_programa)))
                  WHERE (orden_facturacion.id_pago_alumno = cuotas_alumno.id_pago_alumno)) AS numero_cc,
            NULL::text AS num_comprobante_informat
           FROM (cuota
             JOIN cuotas_alumno ON ((cuotas_alumno.id_cuota = cuota.id_cuota)))
          WHERE ((cuota.numero_factura IS NOT NULL) AND (cuota.numero_factura <> ''::text))
        UNION
         SELECT NULL::text AS numero_factura_referenciada,
            2 AS tipo_detalle,
            0 AS iva,
            cuota.id_tipo_documento_tributario,
            ( SELECT concat(rtrim(pago_empresa.nombre_empresa, ' '::text), '######', pago_empresa.rut) AS concat
                   FROM pago_empresa
                  WHERE (pago_empresa.id_pago_empresa = cuotas_empresa.id_pago_empresa)) AS beneficiario_documento,
            cuota.total_cuota AS monto_doc,
            cuota.numero_factura AS numero_doc,
            cuota.fecha_pago AS fecha_lee_doc,
            ( SELECT orden_facturacion.id_of
                   FROM orden_facturacion
                  WHERE (orden_facturacion.id_pago_empresa = cuotas_empresa.id_pago_empresa)) AS id_of,
            ( SELECT programa.numero_cc
                   FROM (programa
                     JOIN orden_facturacion ON ((orden_facturacion.id_programa = programa.id_programa)))
                  WHERE (orden_facturacion.id_pago_empresa = cuotas_empresa.id_pago_empresa)) AS numero_cc,
            NULL::text AS num_comprobante_informat
           FROM (cuota
             JOIN cuotas_empresa ON ((cuotas_empresa.id_cuota = cuota.id_cuota)))
          WHERE ((cuota.numero_factura IS NOT NULL) AND (cuota.numero_factura <> ''::text))
        UNION
         SELECT NULL::text AS numero_factura_referenciada,
            3 AS tipo_detalle,
            0 AS iva,
            pago_otic.id_tipo_documento_tributario,
            concat(rtrim(pago_otic.nombre_otic, ' '::text), '######', pago_otic.rut) AS beneficiario_documento,
            pago_otic.total_otic AS monto_doc,
            pago_otic.numero_factura AS numero_doc,
            pago_otic.fecha_pago AS fecha_lee_doc,
            ( SELECT orden_facturacion.id_of
                   FROM orden_facturacion
                  WHERE (orden_facturacion.id_pago_otic = pago_otic.id_pago_otic)) AS id_of,
            ( SELECT programa.numero_cc
                   FROM (programa
                     JOIN orden_facturacion ON ((orden_facturacion.id_programa = programa.id_programa)))
                  WHERE (orden_facturacion.id_pago_otic = pago_otic.id_pago_otic)) AS numero_cc,
            NULL::text AS num_comprobante_informat
           FROM pago_otic
          WHERE ((pago_otic.numero_factura IS NOT NULL) AND ((pago_otic.numero_factura)::text <> ''::text))
        UNION
         SELECT NULL::text AS numero_factura_referenciada,
            4 AS tipo_detalle,
            cpe_solicitud_facturacion.iva,
            cpe_solicitud_facturacion.id_tipo_documento_tributario,
            concat(rtrim(cpe_solicitud_facturacion.nombre_empresa, ' '::text), '######', cpe_solicitud_facturacion.rut_empresa) AS beneficiario_documento,
            COALESCE(cpe_solicitud_facturacion.monto_neto, (0)::double precision) AS monto_doc,
            cpe_solicitud_facturacion.numero_factura AS numero_doc,
            cpe_solicitud_facturacion.fecha_lee_factura AS fecha_lee_doc,
            cpe_solicitud_facturacion.id_cpe_solicitud_facturacion AS id_of,
            cpe_solicitud_facturacion.numero_cc,
            NULL::text AS num_comprobante_informat
           FROM cpe_solicitud_facturacion
          WHERE ((cpe_solicitud_facturacion.numero_factura IS NOT NULL) AND (cpe_solicitud_facturacion.numero_factura <> ''::text))
        UNION
         SELECT a.numero_factura_referenciada,
            5 AS tipo_detalle,
            0 AS iva,
            'nv'::text AS id_tipo_documento_tributario,
            a.beneficiario_documento,
            sum(a.monto_doc) AS monto_doc,
            a.numero_doc,
            max(a.fecha_lee_doc) AS max,
            a.id_of,
            a.numero_cc,
            a.num_comprobante_informat
           FROM ( SELECT nota_credito_fisica.numero_factura_referenciada,
                    concat(rtrim(nota_credito_fisica.nombre_destinatario, ' '::text), '######', nota_credito_fisica.rut_destinatario) AS beneficiario_documento,
                    nota_credito_fisica.monto AS monto_doc,
                    nota_credito_fisica.numero_nota_credito AS numero_doc,
                    nota_credito_fisica.fecha_lee_doc,
                    nota_credito.id_of,
                    ( SELECT programa.numero_cc
                           FROM (programa
                             JOIN orden_facturacion ON ((orden_facturacion.id_programa = programa.id_programa)))
                          WHERE (orden_facturacion.id_of = nota_credito.id_of)) AS numero_cc,
                    nota_credito_fisica.num_comprobante_informat
                   FROM (nota_credito_fisica
                     JOIN nota_credito ON ((nota_credito_fisica.id_nota_credito = nota_credito.id_nota_credito)))
                  WHERE ((nota_credito_fisica.monto >= 0) AND (nota_credito.id_estado_nota_credito = 7) AND (nota_credito_fisica.numero_nota_credito IS NOT NULL) AND (nota_credito_fisica.numero_nota_credito <> ''::text))) a
          GROUP BY a.numero_factura_referenciada, a.beneficiario_documento, a.numero_doc, a.id_of, a.numero_cc, a.num_comprobante_informat
        UNION
         SELECT NULL::text AS numero_factura_referenciada,
            7 AS tipo_detalle,
            0 AS iva,
            'factura'::text AS id_tipo_documento_tributario,
            a.beneficiario_documento,
            sum(a.monto_doc) AS monto_doc,
            a.numero_doc,
            max(a.fecha_lee_doc) AS max,
            a.id_of,
            a.numero_cc,
            a.num_comprobante_informat
           FROM ( SELECT concat(rtrim(nota_credito_fisica.nombre_destinatario, ' '::text), '######', nota_credito_fisica.rut_destinatario) AS beneficiario_documento,
                    abs(nota_credito_fisica.monto) AS monto_doc,
                    nota_credito_fisica.numero_nota_credito AS numero_doc,
                    nota_credito_fisica.fecha_lee_doc,
                    nota_credito.id_of,
                    ( SELECT programa.numero_cc
                           FROM (programa
                             JOIN orden_facturacion ON ((orden_facturacion.id_programa = programa.id_programa)))
                          WHERE (orden_facturacion.id_of = nota_credito.id_of)) AS numero_cc,
                    nota_credito_fisica.num_comprobante_informat
                   FROM (nota_credito_fisica
                     JOIN nota_credito ON ((nota_credito_fisica.id_nota_credito = nota_credito.id_nota_credito)))
                  WHERE ((nota_credito_fisica.monto < 0) AND (nota_credito.id_estado_nota_credito = 7) AND (nota_credito_fisica.numero_nota_credito IS NOT NULL) AND (nota_credito_fisica.numero_nota_credito <> ''::text))) a
          GROUP BY a.beneficiario_documento, a.numero_doc, a.id_of, a.numero_cc, a.num_comprobante_informat
        UNION
         SELECT ( SELECT string_agg(cpe_factura_referenciada.num_factura, ', '::text ORDER BY cpe_factura_referenciada.num_factura) AS string_agg
                   FROM cpe_factura_referenciada
                  WHERE (cpe_factura_referenciada.id_cpe_nc = cpe_nc.id_cpe_nc)) AS numero_factura_referenciada,
            6 AS tipo_detalle,
            0 AS iva,
            'nv'::text AS id_tipo_documento_tributario,
            concat(rtrim(concat(persona.nombre1, ' ', persona.nombre2, ' ', persona.apellido1, ' ', persona.apellido2), ' '::text), '######', persona.rut) AS beneficiario_documento,
            cpe_nc.monto_nc AS monto_doc,
            cpe_nc.numero_nc AS numero_doc,
            cpe_nc.fecha_lee_doc,
            cpe_nc.id_cpe_nc AS id_of,
            concat(cpe_nc.numero_cc, '_', (date_part('year'::text, cpe_nc.fecha_registro))::text) AS numero_cc,
            cpe_nc.num_comprobante_informat
           FROM (cpe_nc
             JOIN persona ON ((cpe_nc.id_persona_nc = persona.id_persona)))
          WHERE ((cpe_nc.id_cpe_estado_nc = 3) AND (cpe_nc.numero_nc IS NOT NULL) AND (cpe_nc.numero_nc <> ''::text) AND (COALESCE(cpe_nc.monto_nc, (0)::double precision) > (0)::double precision))) v1;]]></view_script>
			<column name="numero_factura_referenciada" type="text" />
			<column name="tipo_detalle" type="integer" />
			<column name="id_tipo_documento_tributario" type="text" />
			<column name="beneficiario_documento" type="text" />
			<column name="monto_doc" type="float8" />
			<column name="numero_doc" type="text" />
			<column name="fecha_lee_doc" type="timestamp" />
			<column name="id_of" type="integer" />
