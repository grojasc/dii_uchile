		<view name="asignacion_gastos_productos_cc" >
			<view_script><![CDATA[CREATE VIEW ${schema}.${name} AS  SELECT a2.id_provedor,
    a2.fecha AS fecha_clase,
    a2.total_clase,
    a2.id_clase,
    a2.id_curso,
    COALESCE(a3.porcentaje, (1)::double precision) AS porcentaje,
    COALESCE(a3.numero_cc, 'Indeterminado'::text) AS numero_cc,
    ((a2.total_clase)::double precision * COALESCE(a3.porcentaje, (1)::double precision)) AS total_ponderado
   FROM (( SELECT a1.id_provedor,
            a1.total_clase,
            a1.id_clase,
            clase.id_curso,
            clase.fecha
           FROM (( SELECT productos.id_provedor,
                    i.id_clase,
                    sum((productos.precio * i.total)) AS total_clase
                   FROM (v_suma_productos_de_clase i
                     JOIN productos ON ((productos.id_producto = i.id_producto)))
                  GROUP BY i.id_clase, productos.id_provedor) a1
             JOIN clase ON ((a1.id_clase = clase.id_clase)))) a2
     LEFT JOIN ( SELECT a.id_curso,
            a.numero_cc,
            a.cant_alumnos_cc,
            b.alumnos_totales_curso,
            ((a.cant_alumnos_cc)::double precision / (b.alumnos_totales_curso)::double precision) AS porcentaje
           FROM (( SELECT alumnos_curso_programa_cc.id_curso,
                    alumnos_curso_programa_cc.numero_cc,
                    count(*) AS cant_alumnos_cc
                   FROM alumnos_curso_programa_cc
                  GROUP BY alumnos_curso_programa_cc.id_curso, alumnos_curso_programa_cc.numero_cc) a
             RIGHT JOIN ( SELECT alumnos_curso_programa_cc.id_curso,
                    count(*) AS alumnos_totales_curso
                   FROM alumnos_curso_programa_cc
                  GROUP BY alumnos_curso_programa_cc.id_curso) b ON ((a.id_curso = b.id_curso)))) a3 ON ((a2.id_curso = a3.id_curso)))
  ORDER BY a2.id_curso;]]></view_script>
			<column name="id_provedor" type="integer" />
			<column name="fecha_clase" type="date" />
			<column name="total_clase" type="numeric" />
			<column name="id_clase" type="integer" />
			<column name="id_curso" type="integer" />
			<column name="porcentaje" type="float8" />
			<column name="numero_cc" type="text" />
			<column name="total_ponderado" type="float8" />
		</view>
		<view name="entregar_informe_para_proveedor" >
			<view_script><![CDATA[CREATE VIEW ${schema}.${name} AS  SELECT a1.cantidad,
    a1.id_producto,
    a1.id_clase,
    a1.id_curso,
    a1.fecha,
    a1.dia,
    a1.sala,
    a1.hora,
    a1.lugar,
    a1.codigo_curso,
    a1.nombre_curso,
    a1.precio_unitario,
    a1.nombre,
    a1.nombre_proveedor,
    a1.id_provedor,
    productos_de_clase.guia,
    curso_esta_en_algun_mba(a1.id_curso) AS es_mba
   FROM (( SELECT c.total AS cantidad,
            c.id_producto,
            c.id_clase,
            c.id_curso,
            c.fecha,
            c.dia,
            c.sala,
            c.horario_colacion AS hora,
            c.direccion_sala AS lugar,
            c.codigo_curso,
            c.nombre_curso,
            d.precio AS precio_unitario,
            d.nombre,
            d.nombre_proveedor,
            d.id_provedor
           FROM (( SELECT a.total,
                    a.id_producto,
                    a.id_clase,
                    a.id_curso,
                    a.fecha,
                    a.dia,
                    a.sala,
                    a.horario_colacion,
                    a.direccion_sala,
                    b.codigo_curso,
                    b.nombre_curso
                   FROM (( SELECT v_suma_productos_de_clase.total,
                            v_suma_productos_de_clase.id_producto,
                            clase.id_clase,
                            clase.id_curso,
                            clase.fecha,
                            clase.horario_colacion,
                            clase.direccion_sala,
                            clase.sala,
                            date_part('dow'::text, clase.fecha) AS dia
                           FROM (v_suma_productos_de_clase
