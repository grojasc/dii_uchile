		<view name="alumnos_curso_programa_cc" >
			<view_script><![CDATA[CREATE VIEW ${schema}.${name} AS  SELECT alumno.id_alumno,
    curso.id_curso,
    alumno_pertenece_al_curso(alumno.id_alumno, curso.id_curso) AS id_programa,
    ( SELECT programa.numero_cc
           FROM programa
          WHERE (programa.id_programa = alumno_pertenece_al_curso(alumno.id_alumno, curso.id_curso))) AS numero_cc
   FROM (alumno
     JOIN curso ON ((alumno_pertenece_al_curso(alumno.id_alumno, curso.id_curso) > 0)));]]></view_script>
			<column name="id_alumno" type="integer" />
			<column name="id_curso" type="integer" />
			<column name="id_programa" type="integer" />
			<column name="numero_cc" type="text" />
		</view>
		<view name="alumnos_de_la_of" >
			<view_script><![CDATA[CREATE VIEW ${schema}.${name} AS  SELECT persona.nombre1,
    persona.apellido1,
    persona.nombre2,
    persona.apellido2,
    persona.rut,
    k.id_of,
    persona.id_persona
   FROM (persona
     JOIN ( SELECT op_of.id_va,
            op_of.id_of,
            op_of.fecha_registro,
            orden_validacion_academica.id_va,
