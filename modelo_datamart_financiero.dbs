		<view name="v_resumen_asistencia" >
			<view_script><![CDATA[CREATE VIEW ${schema}.${name} AS  SELECT r.id_alumno,
    r.id_programa,
    r.ano,
    r.mes,
    r.count,
    r.suma_horas,
    persona.nombre1,
    persona.nombre2,
    persona.apellido1,
    persona.apellido2,
    persona.rut,
    persona.sexo
   FROM (( SELECT b.id_alumno,
            date_part('year'::text, b.fecha) AS ano,
            date_part('month'::text, b.fecha) AS mes,
            cursos_programa.id_programa,
            count(*) AS count,
            sum(round(((date_part('epoch'::text, (((('0001-01-01 '::text || (b.hora_termino)::text) || ':00'::text))::timestamp without time zone - ((('0001-01-01 '::text || (b.hora_inicio)::text) || ':00'::text))::timestamp without time zone)) / ((60 * 60))::double precision))::numeric, 2)) AS suma_horas
           FROM (cursos_programa
             JOIN ( SELECT asistencia.id_asistencia,
                    asistencia.id_alumno,
                    asistencia.id_clase,
                    asistencia.id_secretaria,
                    asistencia.fecha_registro,
                    asistencia.comentario,
                    asistencia.uso_huella,
                    clase.id_clase,
                    clase.id_curso,
                    clase.id_profesor,
                    clase.fecha,
                    clase.temas,
                    clase.actividades,
                    clase.hora_inicio,
                    clase.hora_termino,
                    clase.sala,
                    clase.categoria_profesor,
                    clase.horario_colacion,
                    clase.departamento_sala,
                    clase.direccion_sala,
                    clase.check_sala,
                    clase.comentario_check_sala,
                    clase.nueva_sala_post_check,
                    clase.id_profesor2,
                    clase.id_profesor3,
                    clase.id_profesor4,
                    clase.id_profesor5,
                    clase.fecha_check_sala,
                    clase.asistencia_opcional
                   FROM (asistencia
                     JOIN clase ON ((asistencia.id_clase = clase.id_clase)))
                  WHERE (NOT clase.asistencia_opcional)) b(id_asistencia, id_alumno, id_clase, id_secretaria, fecha_registro, comentario, uso_huella, id_clase_1, id_curso, id_profesor, fecha, temas, actividades, hora_inicio, hora_termino, sala, categoria_profesor, horario_colacion, departamento_sala, direccion_sala, check_sala, comentario_check_sala, nueva_sala_post_check, id_profesor2, id_profesor3, id_profesor4, id_profesor5, fecha_check_sala, asistencia_opcional) ON ((cursos_programa.id_curso = b.id_curso)))
          GROUP BY cursos_programa.id_programa, b.id_alumno, (date_part('year'::text, b.fecha)), (date_part('month'::text, b.fecha))) r
     JOIN persona ON ((persona.id_persona = r.id_alumno)))
  ORDER BY ((COALESCE(persona.apellido1, ''::text) || COALESCE(persona.nombre1, ''::text)) || persona.id_persona), r.ano, r.mes;]]></view_script>
