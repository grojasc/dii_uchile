   FROM cpe_solicitud_facturacion
  WHERE ((cpe_solicitud_facturacion.id_cpe_estado_factura = 3) AND (cpe_solicitud_facturacion.id_estado_envio_of_a_softland = 3) AND ((cpe_solicitud_facturacion.numero_factura IS NULL) OR (cpe_solicitud_facturacion.numero_factura = ''::text)));]]></view_script>
