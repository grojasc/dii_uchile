    of.id_of,
    of.fecha_registro,
    of.id_pago_alumno,
    of.id_pago_empresa,
    of.id_pago_otic,
    COALESCE(of.id_vendedor, of.id_ejecutivo_empresa) AS vendedor,
    of.id_vendedor,
    of.id_ejecutivo_empresa,
    COALESCE(( SELECT sum(cuota.total_cuota) AS sum
           FROM (cuota
             JOIN cuotas_alumno ON ((cuota.id_cuota = cuotas_alumno.id_cuota)))
          WHERE ((cuotas_alumno.id_pago_alumno = of.id_pago_alumno) AND (cuota.numero_factura IS NOT NULL) AND (cuota.numero_factura <> ''::text))), (0)::bigint) AS total_facturado_alumno,
    COALESCE(( SELECT sum(cuota.total_cuota) AS sum
           FROM (cuota
             JOIN cuotas_alumno ON ((cuota.id_cuota = cuotas_alumno.id_cuota)))
          WHERE ((cuotas_alumno.id_pago_alumno = of.id_pago_alumno) AND ((cuota.numero_factura IS NULL) OR (cuota.numero_factura = ''::text)))), (0)::bigint) AS total_no_facturado_alumno,
    COALESCE(( SELECT sum(cuota.total_cuota) AS sum
           FROM (cuota
             JOIN cuotas_empresa ON ((cuota.id_cuota = cuotas_empresa.id_cuota)))
          WHERE ((cuotas_empresa.id_pago_empresa = of.id_pago_empresa) AND (cuota.numero_factura IS NOT NULL) AND (cuota.numero_factura <> ''::text))), (0)::bigint) AS total_facturado_empresa,
    COALESCE(( SELECT sum(cuota.total_cuota) AS sum
           FROM (cuota
             JOIN cuotas_empresa ON ((cuota.id_cuota = cuotas_empresa.id_cuota)))
          WHERE ((cuotas_empresa.id_pago_empresa = of.id_pago_empresa) AND ((cuota.numero_factura IS NULL) OR (cuota.numero_factura = ''::text)))), (0)::bigint) AS total_no_facturado_empresa,
    COALESCE(( SELECT COALESCE(pago_otic.total_otic, 0) AS "coalesce"
           FROM pago_otic
          WHERE ((pago_otic.id_pago_otic = of.id_pago_otic) AND ((pago_otic.numero_factura IS NOT NULL) AND ((pago_otic.numero_factura)::text <> ''::text)))), 0) AS total_facturado_otic,
    COALESCE(( SELECT COALESCE(pago_otic.total_otic, 0) AS "coalesce"
           FROM pago_otic
          WHERE ((pago_otic.id_pago_otic = of.id_pago_otic) AND ((pago_otic.numero_factura IS NULL) OR ((pago_otic.numero_factura)::text = ''::text)))), 0) AS total_no_facturado_otic
   FROM (orden_facturacion of
     JOIN programa ON ((of.id_programa = programa.id_programa)));]]></view_script>
