           FROM (curso
             JOIN clase ON ((curso.id_curso = clase.id_curso)))
          WHERE (NOT clase.asistencia_opcional)) b(id_curso, id_curso_general, descripcion_evaluacion, fecha_registro, codigo_curso, comentario_chequeo_mat_y_serv, id_clase, id_curso_1, id_profesor, fecha, temas, actividades, hora_inicio, hora_termino, sala, categoria_profesor, horario_colacion, departamento_sala, direccion_sala, check_sala, comentario_check_sala, nueva_sala_post_check, id_profesor2, id_profesor3, id_profesor4, id_profesor5, fecha_check_sala, asistencia_opcional, id_cursoo) ON ((cursos_programa.id_curso = b.id_cursoo)))
  GROUP BY cursos_programa.id_programa;]]></view_script>
